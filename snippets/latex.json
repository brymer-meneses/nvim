{
  "DisplayMath": {
    "prefix": "dm",
    "body": ["\\[", "\t$1", "\\]", "$0"],
    "description": "Display Math"
  },
  "InlineMath": {
    "prefix": "im",
    "body": ["\\($1\\)$0"],
    "description": "Inline Math"
  },
  "AlignAt": {
    "prefix": "aa",
    "body": [
      "\\begin{alignat}{2}",
      "\t&&\\hspace{1cm} $1  &&= $2",
      "\t&&\\implies $3 &&= $4",
      "\\end{alignat}",
      "$0"
    ]
  },
  "Image": {
    "prefix": "image",
    "body": [
      "\\begin{figure}[H]",
      "\t\\centering",
      "\t\\includegraphics[scale=$1]{$2}",
      "\\end{figure}",
      "$0"
    ]
  },
  "Table": {
    "prefix": "table",
    "body": [
      "\\begin{table}[H]",
      "\\centering",
      "\t\\begin{tabular}{$1}",
      "\t\\hline",
      "\t\\hline",
      "\t\\end{tabular}",
      "\\end{table}"
    ]
  },
  "Boilerplate": {
    "prefix": "boilerplate",
    "body": [
      "\\documentclass[a4paper]{article}",
      "",
      "\\usepackage{",
      "\t amsfonts",
      "\t amsmath,",
      "\t amssymb,",
      "\t geometry,",
      "\t enumitem,",
      "\t tikz,",
      "\t siunitx,",
      "\t float,",
      "\t tabularx,",
      "\t cancel,",
      "\t fancyhdr",
      "}",
      "",
      "\\date{}",
      "\\setlength{\\parindent}{0pt}",
      "\\geometry{margin=2cm}",
      "\\title{\\bf $1}",
      "\\author{Brymer Bernard Meneses}",
      "",
      "\\newcolumntype{R}[1]{ >{\\raggedright\\arraybackslash}m{#1} }",
      "\\newcolumntype{C}[1]{ >{\\centering\\arraybackslash}m{#1} }",
      "\\newcolumntype{L}[1]{ >{\\raggedleft\\arraybackslash}m{#1} }",
      "",
      "\\newcommand{\\lrp}[1]{",
      "\t\\left( #1 \\right)",
      "}",
      "",
      "% header",
      "\\fancyhf{}",
      "\\pagestyle{fancy}",
      "\\renewcommand{\\footrulewidth}{0.4pt}",
      "\\cfoot{\\thepage}",
      "\\lhead{$1}",
      "\\rhead{Brymer Bernard Meneses}",
      "",
      "\\begin{document}",
      "\\maketitle",
      "$2",
      "\\end{document}"
    ]
  }
}
